}
.top .van-nav-bar {
  vertical-align: middle !important;
  background-color: #16c7ae !important;
  color: #fff !important;
  padding-top: 0px !important;
}
.top .van-nav-bar__title {
  color: #fff !important;
}
.top .van-nav-bar__text {
  color: #fff !important;
}
.top van-icon {
  vertical-align: middle !important;
}
.top .van-nav-bar__arrow {
  color: #fff !important;
}
.top .van-nav-bar .van-icon {
  color: #fff !important;
}
.top .van-nav-bar__right {
  height: 100%;
}
.top van-sidebar {
  height: 100vh;
  overflow: auto;
}
.top_right {
  padding: 20rpx;
  position: fixed;
  right: 10rpx;
  top: 10rpx;
  box-sizing: border-box;
  background-color: rgba(0, 0, 0, 0.4);
  color: #fff;
  border-radius: 10rpx;
}
.center {
  /* padding-top: 100rpx; */
  padding: 20rpx;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  background-color: #fff;
}
.center image {
  width: 240rpx;
  height: 240rpx;
  margin-bottom: 20rpx;
  margin-right: 20rpx;
}
.showpopup {
  text-align: center;
  font-size: 34rpx;
  line-height: 86rpx;
}
.showpopup view {
  border-bottom: 1rpx solid #f2f2f2;
}
.xian {
  width: 100%;
  height: 20rpx;
  background-color: #f2f2f2;
}
.none {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
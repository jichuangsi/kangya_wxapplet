/* pages/W_address/index.wxss */
.W_address {
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
}
.top .van-nav-bar {
  vertical-align: middle !important;
  background-color: #16c7ae !important;
  color: #fff !important;
  padding-top: 0px !important;
}
.top .van-nav-bar__title {
  color: #fff !important;
}
.top .van-nav-bar__text {
  color: #fff !important;
}
.top van-icon {
  vertical-align: middle !important;
}
.top .van-nav-bar__arrow {
  color: #fff !important;
}
.top .van-nav-bar .van-icon {
  color: #fff !important;
}
.top .van-nav-bar__right {
  height: 100%;
}
.top van-sidebar {
  height: 100vh;
  overflow: auto;
}
.center {
  padding-bottom: 150rpx;
  box-sizing: border-box;
  /* background-color: #fff; */
}
.btn {
  width: 100%;
  text-align: center;
  line-height: 86rpx;
  box-sizing: border-box;
  color: #fff;
  border: 20rpx solid #fff;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}
.btn view {
  width: 46%;
  border-radius: 10rpx;
}
.btn .bl {
  background-color: #16c7ae;
}
.btn .br {
  background-color: #0094ff;
}
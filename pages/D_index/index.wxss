swiper {
  height: 400rpx;
}
swiper-item navigator{
  width: 100%;
  height: 100%;
  position: relative;
}
swiper-item image {
  max-height: 100%;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.van-grid-item__icon {
  font-size: 160rpx !important;

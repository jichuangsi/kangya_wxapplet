/* pages/Workbench/index.wxss */
.li {
  display: flex;
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f2f2f2;
  align-items: center;
}
.li image {
  width: 66rpx;
  height: 66rpx;
  margin-right: 20rpx;
}
.li .text {
  flex: 1;
}
.box {
  display: flex;
  flex-direction: column;
}
.ky {
  border-radius: 20rpx;
  background-color: #00AAA4;
}
/* pages/Tooth/index.wxss */
.medical {
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
}
.top .van-nav-bar {
  vertical-align: middle !important;
  background-color: #16c7ae !important;
  color: #fff !important;
  padding-top: 0px !important;
}
.top .van-nav-bar__title {
  color: #fff !important;
}
.top .van-nav-bar__text {
  color: #fff !important;
}
.top van-icon {
  vertical-align: middle !important;
}
.top .van-nav-bar__arrow {
  color: #fff !important;
}
.top .van-nav-bar .van-icon {
  color: #fff !important;
}
.top .van-nav-bar__right {
  height: 100%;
}
.top van-sidebar {
  height: 100vh;
  overflow: auto;
}
.center {
  /* margin-bottom: 100rpx; */
  background-color: #fff;
  position: relative;
}
.li_top {
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}
.li_top image {
  width: 65rpx;
  height: 65rpx;
}
.li_top .name {
  flex: 1;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.user {
  display: flex;
}
.user view {
  flex: 1;
  text-align: center;
  border-left: 1rpx solid #f2f2f2;
  line-height: 66rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.user view .van-icon {
  margin-right: 20rpx;
}
.user view:first-child{
  border: none;
}
.tab {
  border: 1rpx solid #f2f2f2;
  display: flex;
  line-height: 46rpx;
}
.tab .left {
  width: 240rpx;
  border-right: 1rpx solid #f2f2f2;
  text-align: center;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  background-color: #f9f9f9;
}
.tab .right {
  padding: 10rpx 20rpx;
}
.van-dialog__button {
  flex: none;
  width: 50%;
}
.btn {
  width: 100%;
  height: 106rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  border: 20rpx solid #fff;
  background-color: #16c7ae;
  box-sizing: border-box;
}
.box {
  width: 100%;
  height: 60vh;
  position: relative;
}
.y {
  width: 46rpx;
  height: 46rpx;
  text-align: center;
  line-height: 46rpx;
  border-radius: 50%;
  border: 1rpx solid #f2f2f2;
  position: absolute;
}
.check{
  background-color: #16c7ae;
  color: #fff;
}

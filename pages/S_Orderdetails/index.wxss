/* pages/S_Orderdetails/index.wxss */
.S_Orderdetails {
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
  overflow-y: auto;
}
.top {
  width: 100%;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9;
  background-color: #16c7ae;
}
.top .van-nav-bar {
  vertical-align: middle !important;
  background-color: #16c7ae !important;
  color: #fff !important;
  padding-top: 0px !important;
}
.top .van-nav-bar__title {
  color: #fff !important;
}
.top .van-nav-bar__text {
  color: #fff !important;
}
.top van-icon {
  vertical-align: middle !important;
}
.top .van-nav-bar__arrow {
  color: #fff !important;
}
.top .van-nav-bar .van-icon {
  color: #fff !important;
}
.top .van-nav-bar__right {
  height: 100%;
}
.top van-sidebar {
  height: 100vh;
  overflow: auto;
}
.center {
  padding: rpx 0rpx;
  box-sizing: border-box;
}
.center_top {
  width: 100%;
  height: 106rpx;
  background-color: #0094ff;
  color: #fff;
  display: flex;
  align-items: center;
  text-indent: 40rpx;
}
.wl {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  background-color: rgb(216, 216, 252);
}
.white {
  background-color: #fff;
}
.wl .text {
  flex: 1;
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  line-height: 46rpx;
}
.wl .text .time {
  font-size: 24rpx;
  margin-top: 10rpx;
}
.fb {
  display: flex;
  justify-content: flex-end;
}
.mini {
  padding: 5rpx 10rpx;
  border: 1rpx solid #c3c3c3;
  font-size: 26rpx;
}
.box {
  margin-top: 20rpx;
  background-color: #fff;
}
.li {
  width: 100%;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f2f2f2;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  font-size: 28rpx;
}
.li_list {
  width: 100%;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  background-color: #fff;
}
van-card {
  width: 100%;
}
.right {
  justify-content: flex-end;
  border: none;
}
.right view {
  padding: 2rpx 10rpx;
  border: 1rpx solid #333;
  margin-left: 20rpx;
}
.van-dialog__button {
  flex: none;
  width: 50%;
}
.bottom {
  position: fixed;
  bottom: 0rpx;
  left: 0rox;
}
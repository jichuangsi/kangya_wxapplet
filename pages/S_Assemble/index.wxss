/* pages/S_Assemble/index.wxss */
.S_Assemble {
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
  overflow-y: auto;
}
.top {
  width: 100%;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9;
  background-color: #16c7ae;
}
.top .van-nav-bar {
  vertical-align: middle !important;
  background-color: #16c7ae !important;
  color: #fff !important;
  padding-top: 0px !important;
}
.top .van-nav-bar__title {
  color: #fff !important;
}
.top .van-nav-bar__text {
  color: #fff !important;
}
.top van-icon {
  vertical-align: middle !important;
}
.top .van-nav-bar__arrow {
  color: #fff !important;
}
.top .van-nav-bar .van-icon {
  color: #fff !important;
}
.top .van-nav-bar__right {
  height: 100%;
}
.top van-sidebar {
  height: 100vh;
  overflow: auto;
}
.center {
  padding: 76rpx 0rpx;
  box-sizing: border-box;
}
.libox {
  padding: 20rpx;
  box-sizing: border-box;
}
.li {
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f2f2f2;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  font-size: 28rpx;
}
van-card {
  width: 100%;
}
.right {
  justify-content: flex-end;
  border: none;
}
.right view {
  padding: 2rpx 10rpx;
  border: 1rpx solid #333;
  margin-left: 20rpx;
}
.van-dialog__button {
  flex: none;
  width: 50%;
}
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph2" canvas-id="mychart-graph2" ec="{{ market2 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>性别分布</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph3" canvas-id="mychart-graph3" ec="{{ market3 }}"></ec-canvas>
        </view>
      </view>

        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph1" canvas-id="mychart-graph1" ec="{{ hpower1 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>咨询初诊情况</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph2" canvas-id="mychart-graph2" ec="{{ hpower2 }}"></ec-canvas>
        </view>

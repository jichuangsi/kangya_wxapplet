<!--pages/erocdetails/index.wxml-->

<view class="erocdetails">
  <!-- <view class="top">
    <van-nav-bar title="{{title}}" left-text="返回" left-arrow bind:click-left="onClickLeft">
    </van-nav-bar>
  </view> -->

  <view class="center">
    <view class="a1" wx:if="{{title=='每日看板'}}">
      <view class="time" bindtap="oneday">
        {{time}}
        <van-icon name="arrow-down" />
      </view>
      <view class="box" data-text="今日初诊成交总额" bindtap="ontitle">
        <view class="boxtop">
        今日初诊成交总额（元）
        </view>
        <view class="boxcenter">
        {{Daily1.todaydeal}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily1.yesterdaydeal}} 丨 成交率 {{Daily1.dealrate}}
        </view>
      </view>
      <view class="box" data-text="今日诊疗收入" bindtap="ontitle">
        <view class="boxtop">
        今日诊疗收入（元）
        </view>
        <view class="boxcenter">
        {{Daily3.todayincome}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily3.yesterdayincome}}
        </view>
      </view>
      <view class="box" data-text="今日现金流入" bindtap="ontitle">
        <view class="boxtop">
        今日现金流入（元）
        </view>
        <view class="boxcenter">
        {{Daily.todayincome}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily.yesterdayincome}}
        </view>
      </view>
      <view class="box" data-text="今日欠款总额" bindtap="ontitle">
        <view class="boxtop">
        今日欠款总额（元）
        </view>
        <view class="boxcenter">
        {{Daily4.todaydebts}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily4.yesterdaydebts}}
        </view>
      </view>
      <view class="box" data-text="今日新增预交款总额" bindtap="ontitle">
        <view class="boxtop">
        今日新增预交款总额（元）
        </view>
        <view class="boxcenter">
        {{Daily.advancepay}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily.yesterdayadvancepay}}
        </view>
      </view>
      <view class="box" data-text="今日退款总额" bindtap="ontitle">
        <view class="boxtop">
        今日退款总额（元）
        </view>
        <view class="boxcenter">
        {{Daily5.todayrefund}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily5.yesterdayrefund}}
        </view>
      </view>
      <view class="box" data-text="今日预约人数" bindtap="ontitle">
        <view class="boxtop">
        今日预约人数（人次）
        </view>
        <view class="boxcenter">
        {{Daily2.totallost}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily2.firstdeal}}
        </view>
      </view>
      <view class="box" data-text="今日初诊人数" bindtap="ontitle">
        <view class="boxtop">
        今日初诊人数（人次）
        </view>
        <view class="boxcenter">
        {{Daily2.firstvisit}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily2.yesterdayfirstvisit}} 丨 初诊率 {{Daily2.firstvisitrate}}
        </view>
      </view>
      <view class="box" data-text="今日复诊人数" bindtap="ontitle">
        <view class="boxtop">
        今日复诊人数（人次）
        </view>
        <view class="boxcenter">
        {{Daily2.refralvisit}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily2.yesterdayrefralvisit}}
        </view>
      </view>
      <view class="box" data-text="今日流失数" bindtap="ontitle">
        <view class="boxtop">
        今日流失数（人次）
        </view>
        <view class="boxcenter">
        {{Daily2.lostvisitstate}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily2.yesterdaytotallost}} 丨 流失率 {{Daily2.lostvisitrate}}
        </view>
      </view>
      <view class="box" data-text="今日新开会员卡数" bindtap="ontitle">
        <view class="boxtop">
        今日新开会员卡数（张）
        </view>
        <view class="boxcenter">
        {{Daily.vipcardnum}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily.yesterdayvipcardnum}}
        </view>
      </view>
      <view class="box" data-text="今日会员充值总额" bindtap="ontitle">
        <view class="boxtop">
        今日会员充值总额（元）
        </view>
        <view class="boxcenter">
        {{Daily.vipcardfee}}
        </view>
        <view class="boxbottom">
        昨日 {{Daily.yesterdayvipcardfee}}
        </view>
      </view>
      <view class="h3">
        <image src="../../../images/my_icon_Praise.png"></image>
        今日TOP指数
      </view>
      <view class="li">
        <view>今日销售项目TOP3</view>
        <view class="top_li" wx:for="{{Daily_rank1}}" data-text="今日销售项目" data-child="{{item.handlename}}" child_id="{{item.bdhandlesetidentity}}"  bindtap="ontitle">
          <view>{{index+1}}</view>
          <view>{{item.handlename}}</view>
        </view>
      </view>
      <view class="li">
        <view>今日医生业绩TOP3</view>
        <view class="top_li" wx:for="{{Daily_rank2}}" data-text="今日医生业绩" child_id="{{item.doctorid}}" data-child="{{item.bhdoct}}" bindtap="ontitle">
          <view>{{index+1}}</view>
          <view>{{item.bhdoct}}</view>
        </view>
      </view>
      <view class="li">
        <view>今日咨询师业绩TOP3</view>
        <view class="top_li" wx:for="{{Daily_rank3}}" data-text="今日咨询师业绩" data-child="{{item.counselor}}"  bindtap="ontitle">
          <view>{{index+1}}</view>
          <view>{{item.counselor}}</view>
        </view>
      </view>
    </view>
    <view class="a2" wx:if="{{title=='就诊分析'}}">
      <view class="center_top">
        <view class="left">
          <view class="{{check_num == 0?'check':''}}" data-index="0" bindtap="checkclick">日</view>
          <view class="{{check_num == 1?'check':''}}" data-index="1" bindtap="checkclick">月</view>
        </view>
        <view class="right" bindtap="oneday">{{time}}</view>
      </view>
      <view class="jznav">
        <view class="nav_li">
          <view>就诊总人数</view>
          <view>{{attendance.totalvisit}}</view>
        </view>
        <view class="nav_li">
          <view>流失总人数</view>
          <view>{{attendance.totallost}}</view>
        </view>
        <view class="nav_li">
          <view>成交总金额</view>
          <view>{{attendance.totalfee}}</view>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>初诊人次 {{attendance_total1.visit}}</view>
          <view>流失人次 {{attendance_total1.lost}}</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph1" canvas-id="mychart-graph1" ec="{{ attendance1 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>初诊成交总额 {{attendance_total1.free}}</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph2" canvas-id="mychart-graph2" ec="{{ attendance2 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>复诊人次{{attendance_total2.visit}}</view>
          <view>复诊成交总额 {{attendance_total2.free}}</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph3" canvas-id="mychart-graph3" ec="{{ attendance3 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>新诊人次 {{attendance_total3.visit}}</view>
          <view>流失人次 {{attendance_total3.lost}}</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph4" canvas-id="mychart-graph4" ec="{{ attendance4 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>新诊成交总额 {{attendance_total3.free}}</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph5" canvas-id="mychart-graph5" ec="{{ attendance5 }}"></ec-canvas>
        </view>
      </view>
      <view class="h3">
        <image src="../../../images/my_icon_Praise.png"></image>
        今日TOP指数
      </view>
      <view class="li">
        <view>项目销售数量TOP10</view>
        <view class="top_li" wx:for="{{attendance_rank1}}" data-text="项目销售数量" data-child="{{item.handlename}}" child_id="{{item.bdhandlesetidentity}}" bindtap="ontitle">
          <view>{{index+1}}</view>
          <view>{{item.handlename}}</view>
        </view>
      </view>
      <view class="li">
        <view>项目销售金额TOP10</view>
        <view class="top_li" wx:for="{{attendance_rank2}}" data-text="项目销售金额" data-child="{{item.handlename}}" child_id="{{item.bdhandlesetidentity}}" bindtap="ontitle">
          <view>{{index+1}}</view>
          <view>{{item.handlename}}</view>
        </view>
      </view>
    </view>
    <view class="a3" wx:if="{{title=='预约分析'}}">

        <view class="left">
          <view class="{{check_num == 0?'check':''}}" data-index="0" bindtap="checkclick">日</view>
          <view class="{{check_num == 1?'check':''}}" data-index="1" bindtap="checkclick">月</view>
        </view>
        <view class="right" bindtap="oneday">{{time}}</view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>总咨询人数0人</view>
          <view>成交人数0人</view>
          <view>未成交人数0人</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph1" canvas-id="mychart-graph1" ec="{{ consult1 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>成交金额统计0.00</view>
          <view>客单成交额0.00</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph2" canvas-id="mychart-graph2" ec="{{ consult2 }}"></ec-canvas>

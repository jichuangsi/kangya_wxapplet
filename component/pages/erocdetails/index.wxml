        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph4" canvas-id="mychart-graph4" ec="{{ hpower4 }}"></ec-canvas>
        </view>
      </view>
      <view class="libox">
        <view class="libox_title">
          <view>咨询项目分析</view>
        </view>
        <view class="canvas_box">
          <ec-canvas id="mychart-dom-graph5" canvas-id="mychart-graph5" ec="{{ hpower5 }}"></ec-canvas>
